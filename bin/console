#!/usr/bin/env ruby

require 'bundler/setup'
require 'humidifier'

if ENV['TEST']
  require 'aws-sdk'
  security_group = Humidifier::EC2::SecurityGroup.new(group_description: 'Test')
  resources = { 'SecurityGroup' => security_group }

  @stack = Humidifier::Stack.new(name: 'Test-Stack', resources: resources)
end

require 'irb'
IRB.start
