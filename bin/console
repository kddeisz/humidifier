#!/usr/bin/env ruby

require 'bundler/setup'
require 'humidifier'

if ENV['TEST']
  require 'aws-sdk'
  security_group = Humidifier::EC2::SecurityGroup.new(group_description: 'Test')
  @stack = Humidifier::Core::Stack.new(name: 'Test-Stack', resources: { 'SecurityGroup' => security_group })
end

require 'irb'
IRB.start
